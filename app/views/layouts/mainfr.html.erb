<!DOCTYPE html>
<html>
  <head>
    <title>Resalto</title>
    <%= csrf_meta_tags %>
    <%= action_cable_meta_tag %>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= favicon_link_tag 'favicon.ico' %>

  </head>
  <body>
   <%= link_to nl_path( param_cont: params[:controller], param_act: params[:action]), style: "float: right; padding-top: 20px; padding-right: 20px;" do %>
     <%= image_tag 'dutch.jpeg',  class:'img-fluid', width: '30' %>
     <% end %>
  <div class="container">

    <%= render 'shared/navbar_fr' %>
    <%= render 'shared/flashes' %>
    <%= yield %>
    <%= javascript_include_tag 'application' %>
        <%= cloudinary_js_config %>

    </div>
  </body>
</html>
